# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:4200

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Kinesis Configuration
KINESIS_STREAM_NAME=analytics-events

# DynamoDB Configuration
DYNAMODB_TABLE_PREFIX=analytics_

# S3 Configuration
S3_EVENTS_BUCKET=analytics-events-store
S3_ANALYTICS_BUCKET=analytics-results-store

# Athena Configuration
ATHENA_DATABASE=analytics_db
ATHENA_WORKGROUP=primary
ATHENA_OUTPUT_LOCATION=s3://analytics-athena-results/

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Logging
LOG_LEVEL=debug
