# Price Transparency Service Environment Variables

# Server
PORT=3010
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/price_transparency_db

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting (optional overrides)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
MRF_RATE_LIMIT_MAX=10
COMPLIANCE_RATE_LIMIT_MAX=20

# AWS S3 (for MRF file storage)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
MRF_BUCKET_NAME=

# Logging
LOG_LEVEL=info
