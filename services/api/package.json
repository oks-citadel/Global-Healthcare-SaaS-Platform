{
  "name": "@unified-health/api",
  "version": "1.0.0",
  "private": true,
  "description": "Unified Health API Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "tsx prisma/seed-production.ts",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:validate": "tsx scripts/db-health-check.ts",
    "db:backup": "bash scripts/backup-database.sh",
    "db:backup:schema": "bash scripts/backup-database.sh schema-only",
    "db:restore": "bash scripts/restore-database.sh",
    "db:reset": "bash scripts/reset-db.sh"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "@aws-sdk/client-kms": "^3.400.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-secrets-manager": "^3.400.0",
    "@aws-sdk/client-ses": "^3.400.0",
    "@aws-sdk/client-sns": "^3.966.0",
    "@aws-sdk/client-sqs": "^3.400.0",
    "@aws-sdk/client-xray": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.19.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-express": "^0.35.0",
    "@opentelemetry/instrumentation-http": "^0.46.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "@prisma/client": "^5.7.1",
    "@prisma/instrumentation": "^7.2.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/bull": "^4.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/redis": "^4.0.10",
    "apn": "^2.2.0",
    "aws-xray-sdk-core": "^3.6.0",
    "aws-xray-sdk-express": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "prom-client": "^15.1.0",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.6.1",
    "stripe": "^20.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^13.0.0",
    "validator": "^13.15.26",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.12.0",
    "@eslint/js": "^9.18.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^9.18.0",
    "typescript-eslint": "^8.50.1",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
