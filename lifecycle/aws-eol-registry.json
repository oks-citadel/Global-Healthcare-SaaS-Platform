  "riskClassification": {
    "green": {
      "description": "More than 365 days until EOL",
      "action": "Monitor and plan upgrade during next planning cycle",
      "icon": "check-circle"
    },
    "yellow": {
      "description": "Between 180-365 days until EOL",
      "action": "Schedule upgrade within next quarter",
      "icon": "alert-triangle"
    },
    "red": {
      "description": "Less than 180 days until EOL",
      "action": "Immediate upgrade planning required",
      "icon": "x-circle"
    },
    "critical": {
      "description": "Already past EOL or in extended support",
      "action": "Emergency upgrade required",
      "icon": "alert-octagon"
    }
  },
