{
  "name": "@healthcare/ui",
  "version": "1.0.0",
  "description": "Shared UI component library for Unified Health Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "scripts": {
    "build": "npx tsup src/index.ts --format cjs,esm --dts --external react --external react-dom",
    "dev": "npx tsup src/index.ts --format cjs,esm --dts --external react --external react-dom --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.27.1",
    "react-day-picker": "^9.12.0",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.2",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "healthcare",
    "design-system",
    "tailwindcss"
  ],
  "author": "Healthcare Platform Team",
  "license": "MIT"
}
