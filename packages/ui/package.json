{
  "name": "@healthcare/ui",
  "version": "1.0.0",
  "description": "Shared UI component library for Unified Healthcare Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "scripts": {
    "build": "npx tsup src/index.ts --format cjs,esm --dts --external react --external react-dom",
    "dev": "npx tsup src/index.ts --format cjs,esm --dts --external react --external react-dom --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.12.0"
  },
  "devDependencies": {

    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "eslint": "^8.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "healthcare",
    "design-system",
    "tailwindcss"
  ],
  "author": "Healthcare Platform Team",
  "license": "MIT"
}
